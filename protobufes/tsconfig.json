{
    "compilerOptions": {
        "target": "ESNext",
        "lib": ["es2022", "dom"],
        "module": "ESNext",
        "moduleResolution": "Node",
        "rootDir": ".",
        "allowJs": true,
        "outDir": "build",
        "esModuleInterop": true,
        "forceConsistentCasingInFileNames": true,
        "strict": true,
        "noImplicitAny": true,
        "skipLibCheck": true
    }
}
  